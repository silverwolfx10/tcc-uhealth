<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="./Bootstrap.php"
		 backupGlobals="false"
		 backupStaticAttributes="false"
		 verbose="true"
		 colors="true"
		 convertErrorsToExceptions="true"
		 convertNoticesToExceptions="true"
		 convertWarningsToExceptions="true"
		 processIsolation="false"
		 stopOnFailure="false"
		 syntaxCheck="true">
	<testsuite name="123i - Domain">
		<directory>./</directory>
	</testsuite>
	   <logging>
	        <log type="testdox-text" target="_reports/testdox/executed.txt"  />
	        <log type="coverage-text" target="_reports/converage-text/converage.txt"  />
	        <log type="coverage-html" target="_reports/converage-html/"  lowUpperBound="50" highLowerBound="80"/>
    </logging>
</phpunit>